
<ul>
<%= form_for @category do |f| %>
<li>
<%= f.label :name %>
<%= f.text_field :name %>
</li>

<li>
<%= f.label :photo_url %>
<%= f.text_field :photo_url %>
</li>

<li>
<%= f.label :description %>
<%= f.text_field :description %>
</li>

<li>
<%=f.submit "UPDATE" %>
</li>
<%end %>
</ul>

<%= form_for @category, method: :delete do |f| %>
<%=f.submit "DELETE" %>
<%end%>
